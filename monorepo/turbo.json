{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env", "tsconfig.json"],
  "pipeline": {
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalEnv": [
    "DISCORD_CLIENT_SECRET",
    "DISCORD_CLIENT_ID",
    "KUBERNETES_NAMESPACE",
    "KNATIVE_BROKER_URL",
    "DATABASE_URL",
    "DISCORD_TOKEN",
    "NODE_ENV",
    "NEXTAUTH_URL",
    "NEXTAUTH_SECRET",
    "VERCEL_URL",
    "PORT"
  ]
}
